	},
	"devDependencies": {
		"@babel/core": "7.*",
		"@babel/preset-env": "7.*",
		"@babel/register": "7.*",
		"@types/babylon": "6.*",
		"@types/node": "13.*",
		"babelify": "10.*",
		"chai": "4.*",
		"gulp": "4.*",
		"gulp-clean": "0.*",
		"gulp-plumber": "1.*",
		"gulp-typescript": "5.*",
		"mocha": "6.*",
		"npm-check-updates": "4.*",
		"nyc": "15.*",
		"tinyify": "2.*",
		"typescript": "3.*"
	},
	"dependencies": {
		"babylon": "6.*"
	},
	"keywords": [
		"inline",
		"import",
		"es6",
		"statement"
	],
	"files": [
		"lib/main.js",
		"lib/main.d.ts"
